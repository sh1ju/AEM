<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Search Courses"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.page.properties,cq.wcm.msm.properties]"
    helpPath="https://www.adobe.com/go/aem_cmp_page_v1"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
	    <layout
	        jcr:primaryType="nt:unstructured"
	        sling:resourceType="granite/ui/components/foundation/layouts/tabs"
	        type="nav"/>
        <items jcr:primaryType="nt:unstructured">
	        <searchcourses
	            jcr:primaryType="nt:unstructured"
	            jcr:title="Search Courses"
	            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
	            <items jcr:primaryType="nt:unstructured">
	                <columns
	                    jcr:primaryType="nt:unstructured"
	                    sling:resourceType="granite/ui/components/foundation/container">
	                    <items jcr:primaryType="nt:unstructured">
	                        <searchcoursestitletext
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                            fieldLabel="Title"
	                            name="./searchCoursesTitleText"/>
	                        <searchcoursesicon
	                            jcr:primaryType="nt:unstructured"
	                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
	                            allowUpload="{Boolean}false"
	                            autoStart="{Boolean}false"
	                            class="cq-droptarget"
	                                fieldLabel="Icon"
	                                fileNameParameter="./fileName"
	                                fileReferenceParameter="./searchCoursesIconReference"
	                                mimeTypes="[image]"
	                                multiple="{Boolean}false"
	                                name="./searchCoursesIcon"
	                                rootPath="/etc/clientlibs/taylorsuniversity/main/img"
	                                title="Logo Image"
	                                uploadUrl="${suffix.path}"
	                                useHTML5="{Boolean}true"/>
	                        </items>
	                    </columns>
	                </items>
	            </searchcourses>
	            <rightcontent
	                jcr:primaryType="nt:unstructured"
	                jcr:title="Right Content"
	                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
	                <items jcr:primaryType="nt:unstructured">
	                    <columns
	                        jcr:primaryType="nt:unstructured"
	                        sling:resourceType="granite/ui/components/foundation/container">
	                        <items jcr:primaryType="nt:unstructured">
	                            <rightcontenttitle
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                fieldLabel="Title"
	                                name="./rightContentTitle"/>
	                            <rightcontentdescription
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                fieldLabel="Description"
	                                name="./rightContentDescription"/>
	                            <rightcontentlink
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="granite/ui/components/coral/foundation/form/pathbrowser"
	                                fieldLabel="Search Results Page Path"
	                                name="rightContentLink"
	                                rootPath="/content"/>
	                            <rightcontentlinktext
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                fieldLabel="Link Text"
	                                name="./rightContentLinkText"/>
	                            <rightcontentlinktexticon
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
	                                allowUpload="{Boolean}false"
	                                autoStart="{Boolean}false"
	                                class="cq-droptarget"
	                                fieldLabel="Link Icon"
	                                fileNameParameter="./fileName"
	                                fileReferenceParameter="./rightContentLinkTextIconReference"
	                                mimeTypes="[image]"
	                                multiple="{Boolean}false"
	                                name="./rightContentLinkTextIcon"
	                                rootPath="/etc/clientlibs/taylorsuniversity/main/img"
	                                title="Logo Image"
	                                uploadUrl="${suffix.path}"
	                                useHTML5="{Boolean}true"/>
	                        </items>
	                    </columns>
	                </items>
	            </rightcontent>
        </items>
    </content>
</jcr:root>
