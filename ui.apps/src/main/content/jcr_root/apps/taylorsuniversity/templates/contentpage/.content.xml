<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Taylors University Content Page Template"
    jcr:primaryType="cq:Template"
    jcr:title="Taylors University Content Page Template"
    allowedPaths="[/content/taylorsuniversity/.*/.*(/.*)?,/content/tu/.*/.*(/.*)?,/content/tc/.*/.*(/.*)?]"
    ranking="{Long}11">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="/apps/taylorsuniversity/components/page/contentpage">
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Phone"
                    width="{Decimal}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Decimal}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
